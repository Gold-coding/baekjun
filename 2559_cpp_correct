#include <iostream>
#include <vector>
using namespace std;

int main() {
    int N, K;
    cin >> N >> K;

    vector<int> temp(N);
    for (int i = 0; i < N; i++) {
        cin >> temp[i];
    }

    int sum = 0;
    for (int i = 0; i < K; i++) {
        sum += temp[i];
    }

    int maxSum = sum;

    for (int i = K; i < N; i++) {
        sum = sum - temp[i - K] + temp[i];
        if (sum > maxSum) {
            maxSum = sum;
        }
    }

    cout << maxSum << endl;

    return 0;
}
