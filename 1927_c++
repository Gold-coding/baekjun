#include <bits/stdc++.h>
using namespace std;

class MinHeap {
private:
    vector<int> heap;

public:
    void insert(int x) {
        heap.push_back(x);
        int idx = heap.size() - 1;

        while (idx > 0) {
            int parent = (idx - 1) / 2;
            if (heap[parent] <= heap[idx]) break;
            swap(heap[parent], heap[idx]);
            idx = parent;
        }
    }

    int top() {
        if (heap.empty()) return 0;
        return heap[0];
    }

    void pop() {
        if (heap.empty()) return;

        heap[0] = heap.back();
        heap.pop_back();

        int idx = 0;
        int size = heap.size();

        while (true) {
            int left = idx * 2 + 1;
            int right = idx * 2 + 2;
            int smallest = idx;

            if (left < size && heap[left] < heap[smallest])
                smallest = left;
            if (right < size && heap[right] < heap[smallest])
                smallest = right;

            if (smallest == idx) break;
            swap(heap[idx], heap[smallest]);
            idx = smallest;
        }
    }

    bool empty() {
        return heap.empty();
    }
};

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;

    MinHeap h;

    while (n--) {
        int x;
        cin >> x;

        if (x == 0) {
            if (h.empty()) cout << 0 << '\n';
            else {
                cout << h.top() << '\n';
                h.pop();
            }
        } else {
            h.insert(x);
        }
    }
}
