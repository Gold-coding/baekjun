#include <stdio.h>
#include <string.h>

int n, m, v;
int a[1001][1001];
int visited[1001];

void dfs(int x) {
    visited[x] = 1;
    printf("%d ", x);
    for (int i = 1; i <= n; i++) {
        if (a[x][i] && !visited[i]) {
            dfs(i);
        }
    }
}

void bfs(int start) {
    int q[1001];
    int front = 0, rear = 0;
    memset(visited, 0, sizeof(visited));
    visited[start] = 1;
    q[rear++] = start;

    while (front < rear) {
        int x = q[front++];
        printf("%d ", x);
        for (int i = 1; i <= n; i++) {
            if (a[x][i] && !visited[i]) {
                visited[i] = 1;
                q[rear++] = i;
            }
        }
    }
}

int main() {
    scanf("%d %d %d", &n, &m, &v);
    for (int i = 0; i < m; i++) {
        int x, y;
        scanf("%d %d", &x, &y);
        a[x][y] = 1;
        a[y][x] = 1;
    }
    dfs(v);
    printf("\n");
    bfs(v);
    return 0;
}
