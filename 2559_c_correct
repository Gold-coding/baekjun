#include <stdio.h>

int main() {
    int N, K;
    scanf("%d %d", &N, &K);

    int temp[100000];
    for (int i = 0; i < N; i++) {
        scanf("%d", &temp[i]);
    }

    int sum = 0;
    for (int i = 0; i < K; i++) {
        sum += temp[i];
    }

    int maxSum = sum;

    for (int i = K; i < N; i++) {
        sum = sum - temp[i - K] + temp[i];
        if (sum > maxSum) {
            maxSum = sum;
        }
    }

    printf("%d\n", maxSum);
    return 0;
}
